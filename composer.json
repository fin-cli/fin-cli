{
	"name": "wp-cli/wp-cli",
	"description": "A command line interface for WordPress",
	"keywords": [ "cli", "wordpress" ],
	"homepage": "http://wp-cli.org",
	"license": "MIT",
	"bin": [
		"bin/wp.bat", "bin/wp"
	],
	"require": {
		"php": ">=5.3.29",
		"wp-cli/php-cli-tools": "~0.11.1",
		"mustache/mustache": "~2.4",
		"mustangostang/spyc": "~0.5",
		"composer/semver": "~1.0",
		"ramsey/array_column": "~1.1",
		"rmccue/requests": "~1.6",
		"symfony/finder": "~3.1",
		"nb/oxymel": "~0.1.0",
		"composer/composer": "^1.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"behat/behat": "~3.0"
	},
	"suggest": {
		"psy/psysh": "Enhanced `wp shell` functionality"
	},
	"autoload": {
		"psr-0": { "WP_CLI": "php" },
		"classmap": [ "php/export" ]
	}
}
