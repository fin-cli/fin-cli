{
	"name": "wp-cli/wp-cli",
	"description": "A command line interface for WordPress",
	"keywords": [ "cli", "wordpress" ], 
	"homepage": "http://wp-cli.org",
	"license": "MIT",
	"bin": [
		"bin/wp"
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://wp-cli.org/package-index/"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"wp-cli/php-cli-tools": "0.9.3",
		"mustache/mustache": "~2.3",
		"composer/composer": "dev-master"
	},
	"suggest": {
		"d11wtq/boris": "Enhanced `wp shell` functionality"
	},
	"require-dev": {
		"phpunit/phpunit": "~3.7",
		"behat/behat": "~2.4"
	},
	"autoload": {
		"psr-0": { "WP_CLI": "php" }
	},
	"extra": {
		"installer-paths": {
			"vendor/{$name}": ["wp-cli/php-cli-tools"]
		}
    }
}
